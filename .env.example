#
# Copy to `.env` and fill values.
#

# Required for live trading.
BINANCE_API_KEY=your_binance_api_key
BINANCE_SECRET_KEY=your_binance_secret_key

# Required for testnet trading.
BINANCE_TESTNET_FUTURE_API_KEY=your_testnet_api_key
BINANCE_TESTNET_FUTURE_SECRET_KEY=your_testnet_secret_key

# Optional LLM providers.
OPENAI_API_KEY=your_openai_key
ANTHROPIC_API_KEY=your_anthropic_key

# Optional overrides.
CONFIG_PATH=config.yaml
MONITORING__LOG_LEVEL=INFO
MONITORING__LOG_HTTP=false
MONITORING__LOG_HTTP_RESPONSES=false
RUN_MODE=paper
RUN_ENABLE_TRADING=false
RUN_LIVE_CONFIRM=YES_I_UNDERSTAND
